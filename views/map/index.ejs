<html>
	<head>
		<title>Leaflet Mobile Map</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
	</head>
	<body>
		<div id="map"></div>
		<div id="upload-panel" class="panel panel-default vcenter">
	  	<div class="panel-heading upload-panel-header"></div>
	  	<div class="panel-body" id="upload-panel-body" >
	    	<form action="/doc/fileUpload" enctype="multipart/form-data" method="post" id="file-form">
	    		<input type="hidden" name="_csrf" value="<%= _csrf %>" />
	    		<!-- Select File Button -->
				<div class="input-group" id="file-upload">
					<div class="form-group">
						<input type="file" name="uploadFile" id="Filebtninfo">
					</div>
				</div><br/>
				
		    	<input type="submit" value="Submit" class="btn btn-primary" style="display: block; margin: 0 auto"/>
	    	</form>
	    	
	    	<form action="#" enctype="multipart/form-data" method="post" id="addMarker-form">
	    		<div class="form-group">
				  <label for="title">Title:</label>
				  <input type="text" class="form-control" id="title">
				</div>
				<div class="form-group">
				  <label for="firstName">First Name:</label>
				  <input type="text" class="form-control" id="firstName">
				</div>
				<div class="form-group">
				  <label for="lastName">Last Name:</label>
				  <input type="text" class="form-control" id="lastName">
				</div>
				<div class="form-group">
				  <label for="address">Address:</label>
				  <input type="text" class="form-control" id="address">
				</div>
				<div class="form-group">
				    <label for="exampleInputAmount">Quote Amount</label>
				    <div class="input-group">
				      <div class="input-group-addon">$</div>
				      <input type="number" min="0" step="1" class="form-control" id="exampleInputAmount" placeholder="Amount">
				      <div class="input-group-addon">.00</div>
				    </div>
				</div>
				<div class="form-group">
				  <label for="frequency">Frequency:</label>
				  <select class="form-control" id="frequency">
				    <option>Once</option>
				    <option>Weekly</option>
				    <option>Bi-weekly</option>
				    <option>Monthly</option>
				  </select>
				</div>
				<div class="form-group">
				  <label for="contract">Type of Contract:</label>
				  <select class="form-control" id="contract">
				    <option>Contract</option>
				    <option>Subscription</option>
				  </select>
				</div>

				<input type="submit" value="Submit" class="btn btn-primary" style="display: block; margin: 0 auto"/>
				<input type="hidden" name="_csrf" value="<%= _csrf %>" />
	    	</form>
	  	</div>
		  	
		</div>
		
	</body>
</html>
